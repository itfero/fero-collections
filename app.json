{
  "expo": {
    "name": "Fero Collections",
    "slug": "fero-collections",
    "version": "1.0.0",
    "extra": {
      "API_PREFIX": "https://brt30fpab4.execute-api.ap-south-1.amazonaws.com/"
    },
    "scheme": "fero-collections",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/door-slider-bg1.jpg",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0c4ef4"
    },
    "android": {
      "package": "com.ferocollections",
      "launchMode": "singleTask",
      "adaptiveIcon": {
        "foregroundImage": "./assets/FDCollections.png",
        "backgroundColor": "#0c4ef4"
      },
      "allowBackup": "false"
    },
    "ios": {
      "supportsTablet": "true"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "newArchEnabled": "true",
    "platforms": [
      "android",
      "ios",
      "web"
    ]
  }
}